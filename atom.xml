---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>{{ site.title }}</title>
 <link href="http://iseetheline.ru/atom.xml" rel="self"/>
 <link href="http://iseetheline.ru/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://litanyagainstfear.com/</id>
 <author>
   <name>Pavel Popov</name>
   <email>pavelpopov@outlook.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title | xml_escape }}</title>
   <link href="http://iseetheline.ru{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://iseetheline.ru{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
